{
    "version": 4,
    "configurePresets": [
        {
            "name": "default",
            "displayName": "Default",
            "generator": "Unix Makefiles",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "conan",
            "displayName": "Conan",
            "inherits": "default",
            "cacheVariables": {
                "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/build/conan_toolchain.cmake"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "default",
            "configurePreset": "default"
        },
        {
            "name": "conan",
            "configurePreset": "conan"
        }
    ]
}
